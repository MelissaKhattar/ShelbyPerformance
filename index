        ];

        const trendChartData = [
            { "Name": "AtmosphereCleanliness", "01-07 Jun": 72, "08-14 Jun": 78, "15-21 Jun": 74, "22-28 Jun": 76, "29 Jun-05 Jul": 58, "06-12 Jul": 62 },
